{
  "app": {
    "name": "ccxt-position",
    "env": "docker",
    "db_engine": "mysql"
  },
  "database": {
    "engine": "mysql",
    "mysql_driver": "aiomysql",
    "mysql_host": "mysql",
    "mysql_port": 3306,
    "mysql_user": "root",
    "mysql_password": "root",
    "mysql_database": "ccxt_position",
    "mysql_min_pool_size": 1,
    "mysql_max_pool_size": 10
  },
  "worker": {
    "worker_id": "worker-position-0",
    "pool_id": 0,
    "poll_interval_ms": 1000,
    "max_attempts": 5,
    "auto_reconcile_enabled": true,
    "reconcile_short_interval_seconds": 60,
    "reconcile_short_lookback_minutes": 10,
    "reconcile_hourly_interval_seconds": 3600,
    "reconcile_hourly_lookback_minutes": 60,
    "reconcile_long_interval_seconds": 86400,
    "reconcile_long_lookback_days": 10,
    "reconcile_batch_limit": 200
  },
  "dispatcher": {
    "host": "127.0.0.1",
    "port": 9100,
    "request_timeout_seconds": 30,
    "pool_size_ccxt": 8,
    "pool_size_ccxtpro": 4
  },
  "logging": {
    "log_dir": "logs",
    "disable_uvicorn_access_log": true,
    "app_request_log": true
  },
  "security": {
    "encryption_master_key": "",
    "require_encrypted_credentials": false
  }
}
